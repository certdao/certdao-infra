{
  "name": "certdao-infra",
  "version": "1.0.0",
  "description": "server that will download site content looking for contract addresses",
  "type": "module",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -c tslint.json 'checker/**/*.ts'",
    "checker": "yarn run build && node --experimental-specifier-resolution=node dst/index.js",
    "clean": "rm -rf dst",
    "build": "yarn run clean && babel ./checker --copy-files --out-dir dst --extensions '.ts'"
  },
  "dependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/node": "^18.11.4",
    "@types/website-scraper": "^1.2.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "glob": "^8.0.3",
    "helmet": "^6.0.0",
    "node-fetch": "^3.2.10",
    "tsc-node": "^0.0.3",
    "tslint": "^6.1.3",
    "tslog": "^3.3.4",
    "web3-utils": "^1.8.0",
    "website-scraper": "^5.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/glob": "^8.0.0",
    "tsc": "^2.0.4",
    "typescript": "^4.8.4"
  }
}
